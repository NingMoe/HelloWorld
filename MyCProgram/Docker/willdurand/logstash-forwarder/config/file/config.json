{
  "network": {
    "servers": [ "pf.hive.sobey.com:5043" ],
    "ssl certificate": "/etc/ssl/logstash-forwarder.crt",
    "ssl key": "/etc/ssl/logstash-forwarder.key",
    "ssl ca": "/etc/ssl/ca.crt"
  },
  "files": [
    {
      "paths":  [ "/var/log/nginx/access.log" ],
      "fields": { "type": "nginx" }
    }
  ]
